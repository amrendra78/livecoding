{
  "name": "livecoding",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
  "start": "ng serve",
  "prebuild": "node scripts/generate-env.js",
  "build": "ng build --configuration production",
  "build:ssr": "ng run livecoding:server:production && ng run livecoding:browser:production && node dist/livecoding/server/main.js",
  "postbuild": "echo build finished",
  "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:livecoding": "node dist/livecoding/server/server.mjs",
    "vercel-build": "npm run build"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.2",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.2",
    "@angular/ssr": "^20.3.3",
  "bcryptjs": "^3.0.2",
    "express": "^5.1.0",
  "mongoose": "^7.5.0",
  "serverless-http": "^2.1.0",
  "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "@supabase/supabase-js": "^2.27.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~20.3.3",
    "@angular/build": "^20.3.3",
